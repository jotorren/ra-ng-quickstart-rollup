{
  "name": "ra-ng-quickstart-rollup",
  "version": "2.0.0",
  "description": "ra-ng project quickstart with systemjs/rollup",
  "scripts": {
    "start": "rimraf dist && tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server -c liteserver.json",
    "server": "http-server dist/public --cors",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "tsc:prod": "tsc -p tsconfig.prod.json",
    "ngc":"ngc -p tsconfig.prod-aot.json",
    "tslint": "tslint -c tslint.json src/app/**/*.ts",
    "test": "rimraf dist && tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "test-once": "rimraf dist && tsc && karma start karma.conf.js --single-run",
    "e2e": "protractor protractor.config.js",
    "webdriver:update": "webdriver-manager update",
    "karma": "karma start karma.conf.js",
    "rollup": "rollup -c rollup.config.js",
    "rollup:map": "rollup -c rollup.config.js && source-map-explorer dist/public/app.js",
    "rollup:aot": "rollup -c rollup.config.js",
    "rollup:aot:map": "rollup -c rollup.config.js && source-map-explorer dist/public/app.js"
  },
  "keywords": [
    "angular2",
    "ra-ng",
    "systemjs",
    "aot",
    "rollup"
  ],
  "author": "ICT Analysis, Architecture & Design",
  "license": "Apache-2.0",
  "dependencies": {
    "ra-ng": "2.1.0",
    "@angular/common": "2.4.4",
